<%
@config_hash.keys.sort.each do |k|
  case @config_hash[k].class.to_s
  when 'TrueClass' then
-%>
<%= k %>
<%
  when 'FalseClass' then
    # NOOP
  when 'Array' then
    @config_hash[k].sort.each do |v|
-%>
<%= k %>=<%= v.to_s %>
<%
    end
  else
-%>
<%= k %>=<%= @config_hash[k].to_s %>
<%
  end
end -%>
